{
  "name": "@abraxas/voting-stimmregister-webapp",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:stats": "ng build --configuration=production --stats-json=true",
    "build:prod": "ng build --configuration=production",
    "e2e": "ng e2e",
    "lint": "ng lint && npm run lint:styles",
    "lint:ci": "ng lint --format json > eslint-report.json",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:license": "eslint --fix .",
    "ng": "ng",
    "npx": "npx",
    "prepare": "husky install",
    "prettier": "npx prettier --write ./src/**/*.{ts,scss,html,json}",
    "server": "ng serve voting-stimmregister-webapp -o",
    "start": "ng serve",
    "stylelint-check": "stylelint-config-prettier-check",
    "test": "jest -c jest.config.js --coverage --no-cache",
    "test:all": "jest -c jest.config.js --coverage --detect-open-handles",
    "test:ci": "jest -c jest.config.js --ci --coverage --coverageDirectory coverage/",
    "test:watch": "jest -c jest.config.js --watch --detect-open-handles",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/voting-stimmregister-webapp/stats.json"
  },
  "private": true,
  "dependencies": {
    "@abraxas/base-components": "17.2.0",
    "@abraxas/voting-lib": "4.0.0",
    "@abraxas/voting-stimmregister-proto": "2.0.0",
    "@angular-architects/module-federation": "17.0.8",
    "@angular/animations": "17.2.0",
    "@angular/cdk": "17.2.0",
    "@angular/cdk-experimental": "17.2.0",
    "@angular/common": "17.2.0",
    "@angular/compiler": "17.2.0",
    "@angular/core": "17.2.0",
    "@angular/forms": "17.2.0",
    "@angular/material": "17.2.0",
    "@angular/material-moment-adapter": "17.2.0",
    "@angular/platform-browser": "17.2.0",
    "@angular/platform-browser-dynamic": "17.2.0",
    "@angular/router": "17.2.0",
    "@ngx-translate/core": "14.0.0",
    "bootstrap": "5.2.0",
    "core-js": "3.20.3",
    "rxjs": "7.8.1",
    "tslib": "2.4.0",
    "uuid": "^9.0.0",
    "zone.js": "0.14.5"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1702.0",
    "@angular-devkit/build-angular": "17.2.0",
    "@angular-eslint/builder": "17.2.0",
    "@angular-eslint/eslint-plugin": "17.2.0",
    "@angular-eslint/eslint-plugin-template": "17.2.0",
    "@angular-eslint/schematics": "17.2.0",
    "@angular-eslint/template-parser": "17.2.0",
    "@angular/cli": "17.2.0",
    "@angular/compiler-cli": "17.2.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "28.1.6",
    "@types/jsdom": "20.0.0",
    "@types/node": "18.6.5",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "codelyzer": "6.0.2",
    "eslint": "^8.53.0",
    "eslint-plugin-license-header": "0.6.0",
    "http-server": "14.1.1",
    "husky": "8.0.1",
    "jest": "29.7.0",
    "jest-preset-angular": "13.1.4",
    "jest-sonar-reporter": "2.0.0",
    "tough-cookie": "^4.1.3",
    "lint-staged": "13.0.3",
    "ngx-build-plus": "17.0.0",
    "prettier": "2.7.1",
    "shallow-render": "17.0.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "28.0.0",
    "stylelint-config-standard-scss": "5.0.0",
    "stylelint-scss": "4.3.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack-bundle-analyzer": "4.6.1"
  }
